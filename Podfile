# platform :ios, '9.0'

target 'Pokemon' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Pokemon
  pod 'Alamofire'
  pod 'Material', '3.1.8'
  pod 'AlamofireImage', '4.1.0'
  pod "Koloda"
  pod 'RealmSwift', '~>10'


  target 'PokemonTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Swifter', '~> 1.5.0'

  end

  target 'PokemonUITests' do
    # Pods for testing
    pod 'Swifter', '~> 1.5.0'

  end

end
